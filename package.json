{
  "name": "playcount-decky",
  "version": "1.1",
  "description": "A Steam Deck plugin that shows current player counts for your steam games.",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w"
  },
  "keywords": ["decky", "plugin", "steam-deck", "deck"],
  "author": "itsOwen",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@decky/rollup": "^1.0.1",
    "@decky/ui": "^4.8.3",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/webpack": "^5.28.5",
    "rollup": "^4.29.1",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@decky/api": "^1.1.2",
    "react-icons": "^5.4.0",
    "tslib": "^2.8.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom"
      ]
    }
  }
}